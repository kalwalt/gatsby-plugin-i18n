{
  "name": "gatsby-plugin-i18n",
  "version": "1.0.1",
  "description": "Multi language feature for Gatsby",
  "homepage": "https://github.com/angeloocana/gatsby-plugin-i18n/tree/master/packages/gatsby-plugin-i18n#readme",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --fix ./src",
    "prebuild": "yarn run lint",
    "build": "babel src --out-dir . --ignore '**/*.test.js'",
    "watch": "babel -w src --out-dir . --ignore '**/*.test.js'",
    "test": "yarn run lint && jest",
    "coverage": "codecov",
    "test:open": "opn ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloocana/gatsby-plugin-i18n.git",
    "directory": "packages/gatsby-plugin-i18n"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "i18n"
  ],
  "author": "angeloocana.com",
  "license": "MIT",
  "dependencies": {
    "folktale": "^2.3.2",
    "graphql": "^16.3.0",
    "ptz-i18n": "^1.0.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@types/ramda": "^0.28.0",
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.24.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "eslint-plugin-react": "^7.29.2",
    "jest": "^27.4.4",
    "open-cli": "^7.0.1",
    "ptz-assert": "^1.6.8"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testURL": "http://localhost/"
  }
}